<Bindings>
  <Binding name="CLICK HSCollapseButton:LeftButton" description="Collapse Tracker" category="Horizon Suite - Focus">
    if _G.HSCollapseButton and _G.HSCollapseButton:GetScript("OnClick") then _G.HSCollapseButton:GetScript("OnClick")() end
  </Binding>
  <Binding name="CLICK HSNearbyToggleButton:LeftButton" description="Toggle Nearby Group" category="Horizon Suite - Focus">
    if _G.HSNearbyToggleButton and _G.HSNearbyToggleButton:GetScript("OnClick") then _G.HSNearbyToggleButton:GetScript("OnClick")() end
  </Binding>
</Bindings>
